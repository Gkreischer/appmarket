{
  "name": "products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "brand": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "isShow": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
